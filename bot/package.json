{
  "name": "minecraft-bot",
  "version": "1.0.0",
  "description": "A Minecraft bot with LLM-powered task execution",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias && mkdir -p dist/llm/prompts && cp -r src/llm/prompts/* dist/llm/prompts/",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register --project tsconfig.json src/index.ts",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/ws": "^8.5.10",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "lru-cache": "^11.1.0",
    "mineflayer": "^4.20.0",
    "mineflayer-collectblock": "^1.6.0",
    "mineflayer-pathfinder": "^2.4.5",
    "mineflayer-pvp": "^1.3.2",
    "mineflayer-tool": "^1.2.0",
    "node-gzip": "^1.1.2",
    "openai": "^4.28.4",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "prismarine-viewer": "^1.25.0",
    "prom-client": "^15.1.0",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0",
    "vec3": "^0.1.10",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^10.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2"
  }
} 