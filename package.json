{
  "name": "minecraft-bot",
  "version": "1.0.0",
  "description": "A Minecraft bot with LLM-powered task execution",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "docker-compose -f docker-compose.dev.yml up",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:web": "cd src/web && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:web\"",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "web": "node src/web/server.ts",
    "migrate-config": "ts-node src/config/migrate.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs": "^4.17.0",
    "@tensorflow/tfjs-backend-wasm": "^4.17.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "@types/express": "^5.0.1",
    "@xboxreplay/xboxlive-auth": "^4.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "lru-cache": "^11.1.0",
    "mineflayer": "^4.0.0",
    "mineflayer-auto-eat": "^3.3.6",
    "mineflayer-collectblock": "^1.6.0",
    "mineflayer-pathfinder": "^2.4.5",
    "mineflayer-pvp": "^1.3.2",
    "mineflayer-tool": "^1.2.0",
    "ml-cart": "^2.0.0",
    "ml-random-forest": "^2.1.0",
    "morgan": "^1.10.0",
    "node-gzip": "^1.1.2",
    "prismarine-auth": "^2.0.0",
    "prismarine-biome": "^1.3.0",
    "prom-client": "^15.1.3",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "vec3": "^0.1.8",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "concurrently": "^8.2.2"
  }
}
